  "grammars": [
    {
      "name": "nearley",
      "camelcase": "Nearley",
      "title": "Nearley",
      "scope": "source.nearley",
      "file-types": [
        "ne"
      ],
      "injection-regex": "^nearley$",
      "class-name": "TreeSitterNearley"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Nearley grammar for tree-sitter",
    "authors": [
      {
        "name": "mi2ebi",
        "email": "merrybot47@gmail.com"
      }
    ],
    "links": {
      "repository": "https://github.com/mi2ebi/tree-sitter-nearley"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}