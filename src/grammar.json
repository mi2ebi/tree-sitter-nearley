{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "nearley",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "_item"
      }
    },
    "_item": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "directive"
        },
        {
          "type": "SYMBOL",
          "name": "rule"
        },
        {
          "type": "SYMBOL",
          "name": "comment"
        },
        {
          "type": "SYMBOL",
          "name": "cont_block"
        },
        {
          "type": "SYMBOL",
          "name": "ifdef"
        }
      ]
    },
    "comment": {
      "type": "PATTERN",
      "value": "#[^\\n]*"
    },
    "directive": {
      "type": "SEQ",
      "members": [
        {
          "type": "PATTERN",
          "value": "@[a-zA-Z_]\\w*"
        },
        {
          "type": "SYMBOL",
          "name": "directive_value"
        },
        {
          "type": "PATTERN",
          "value": "\\n"
        }
      ]
    },
    "directive_value": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "identifier"
        },
        {
          "type": "SYMBOL",
          "name": "string"
        }
      ]
    },
    "cont_block": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "@{%"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "cont"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "STRING",
          "value": "%}"
        }
      ]
    },
    "cont": {
      "type": "PATTERN",
      "value": "([^%]|%[^}])*"
    },
    "cont_inline": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "{%"
        },
        {
          "type": "SYMBOL",
          "name": "cont"
        },
        {
          "type": "STRING",
          "value": "%}"
        }
      ]
    },
    "rule": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "rule_name"
            },
            {
              "type": "SYMBOL",
              "name": "generic"
            },
            {
              "type": "SYMBOL",
              "name": "macro_name"
            }
          ]
        },
        {
          "type": "STRING",
          "value": "->"
        },
        {
          "type": "SYMBOL",
          "name": "rule_body"
        },
        {
          "type": "PATTERN",
          "value": "\\n"
        }
      ]
    },
    "rule_name": {
      "type": "SYMBOL",
      "name": "identifier"
    },
    "rule_body": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "alternative"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": "|"
              },
              {
                "type": "SYMBOL",
                "name": "alternative"
              }
            ]
          }
        }
      ]
    },
    "macro_name": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "identifier"
        },
        {
          "type": "STRING",
          "value": "["
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "SYMBOL",
              "name": "identifier"
            },
            {
              "type": "REPEAT",
              "content": {
                "type": "SEQ",
                "members": [
                  {
                    "type": "STRING",
                    "value": ","
                  },
                  {
                    "type": "SYMBOL",
                    "name": "identifier"
                  }
                ]
              }
            }
          ]
        },
        {
          "type": "STRING",
          "value": "]"
        }
      ]
    },
    "macro_arg": {
      "type": "PATTERN",
      "value": "\\$[a-zA-Z_]\\w*"
    },
    "alternative": {
      "type": "PREC_RIGHT",
      "value": 0,
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SEQ",
            "members": [
              {
                "type": "REPEAT1",
                "content": {
                  "type": "SYMBOL",
                  "name": "element"
                }
              },
              {
                "type": "SYMBOL",
                "name": "cont_inline"
              }
            ]
          },
          {
            "type": "REPEAT1",
            "content": {
              "type": "SYMBOL",
              "name": "element"
            }
          }
        ]
      }
    },
    "element": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "symbol"
        },
        {
          "type": "SYMBOL",
          "name": "quantified"
        }
      ]
    },
    "symbol": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "identifier"
        },
        {
          "type": "SYMBOL",
          "name": "string"
        },
        {
          "type": "SYMBOL",
          "name": "group"
        },
        {
          "type": "SYMBOL",
          "name": "macro_arg"
        },
        {
          "type": "SYMBOL",
          "name": "charset"
        },
        {
          "type": "SYMBOL",
          "name": "wildcard"
        },
        {
          "type": "SYMBOL",
          "name": "token"
        },
        {
          "type": "SYMBOL",
          "name": "generic"
        }
      ]
    },
    "token": {
      "type": "PATTERN",
      "value": "%[a-zA-Z_]\\w*"
    },
    "generic": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "identifier"
        },
        {
          "type": "STRING",
          "value": "<"
        },
        {
          "type": "SYMBOL",
          "name": "identifier"
        },
        {
          "type": "STRING",
          "value": ">"
        }
      ]
    },
    "quantified": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "symbol"
        },
        {
          "type": "SYMBOL",
          "name": "quantifier"
        }
      ]
    },
    "quantifier": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": ":?"
        },
        {
          "type": "STRING",
          "value": ":*"
        },
        {
          "type": "STRING",
          "value": ":+"
        }
      ]
    },
    "group": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "("
        },
        {
          "type": "SYMBOL",
          "name": "rule_body"
        },
        {
          "type": "STRING",
          "value": ")"
        }
      ]
    },
    "charset": {
      "type": "PATTERN",
      "value": "\\[\\^?([^\\]]|\\\\\\])+\\]"
    },
    "wildcard": {
      "type": "STRING",
      "value": "."
    },
    "ifdef": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "ifdef_start"
        },
        {
          "type": "SYMBOL",
          "name": "identifier"
        },
        {
          "type": "PATTERN",
          "value": "\\n"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "_item"
          }
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "ifdef_else"
                },
                {
                  "type": "REPEAT",
                  "content": {
                    "type": "SYMBOL",
                    "name": "_item"
                  }
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "ifdef_endif"
        }
      ]
    },
    "ifdef_start": {
      "type": "STRING",
      "value": "#ifdef"
    },
    "ifdef_else": {
      "type": "STRING",
      "value": "#else"
    },
    "ifdef_endif": {
      "type": "STRING",
      "value": "#endif"
    },
    "string": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "\""
            },
            {
              "type": "PATTERN",
              "value": "[^\"]*"
            },
            {
              "type": "STRING",
              "value": "\""
            },
            {
              "type": "CHOICE",
              "members": [
                {
                  "type": "STRING",
                  "value": "i"
                },
                {
                  "type": "BLANK"
                }
              ]
            }
          ]
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "'"
            },
            {
              "type": "PATTERN",
              "value": "[^']*"
            },
            {
              "type": "STRING",
              "value": "'"
            },
            {
              "type": "CHOICE",
              "members": [
                {
                  "type": "STRING",
                  "value": "i"
                },
                {
                  "type": "BLANK"
                }
              ]
            }
          ]
        }
      ]
    },
    "identifier": {
      "type": "PATTERN",
      "value": "[a-zA-Z_]\\w*"
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s+"
    },
    {
      "type": "SYMBOL",
      "name": "comment"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}